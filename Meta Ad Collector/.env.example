# Example .env file for collector
# Local Database Environment Variables (for data collection)
LOCAL_PG_HOST=localhost
LOCAL_PG_PORT=5432
LOCAL_PG_USER=your_local_db_user
LOCAL_PG_PASSWORD=your_local_db_password
LOCAL_PG_DATABASE=real_political_ads_db

# AWS RDS Database (for syncing)
PG_HOST=your-rds-endpoint.amazonaws.com
PG_PORT=5432
PG_USER=your_rds_user
PG_PASSWORD=your_rds_password
PG_DATABASE=mydb

# Facebook API Key
FACEBOOK_API_KEY=your_facebook_api_key_here
