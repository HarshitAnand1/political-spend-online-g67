# Production Environment Variables for EC2
# Copy this to .env.production and fill in your values

# Database connection (use private IP if DB is in same VPC)
DATABASE_URL=postgresql://username:password@your-rds-endpoint:5432/political_ads_db

# Node environment
NODE_ENV=production

# Application port
PORT=3000

# Optional: Redis for caching (if you set it up)
# REDIS_URL=redis://your-redis-endpoint:6379
